{
  "kind": "collectionType",
  "collectionName": "egresados",
  "info": {
    "singularName": "egresado",
    "pluralName": "egresados",
    "displayName": "Egresado",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "dni": {
      "type": "string",
      "maxLength": 8
    },
    "nombres": {
      "type": "string"
    },
    "fechaNacimiento": {
      "type": "date"
    },
    "fotografia": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "departamentoNacimiento": {
      "type": "enumeration",
      "enum": [
        "Piura",
        "Lima"
      ]
    },
    "provinciaNacimiento": {
      "type": "enumeration",
      "enum": [
        "Sullana",
        "Talara"
      ]
    },
    "distritoNacimiento": {
      "type": "enumeration",
      "enum": [
        "Talara",
        "Sullana"
      ]
    },
    "departamentoDomicilio": {
      "type": "enumeration",
      "enum": [
        "Piura"
      ]
    },
    "provinciaDomicilio": {
      "type": "enumeration",
      "enum": [
        "Talara"
      ]
    },
    "distritoDomicilio": {
      "type": "enumeration",
      "enum": [
        "Talara"
      ]
    },
    "programas_de_estudios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::programa.programa",
      "mappedBy": "egresado"
    },
    "anioInicio": {
      "type": "integer"
    },
    "anioFin": {
      "type": "integer"
    },
    "periodo": {
      "type": "enumeration",
      "enum": [
        "I",
        "II",
        "III",
        "IV",
        "V",
        "VI"
      ]
    },
    "creditosAprobados": {
      "type": "integer"
    },
    "titulo_profesionals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::titulo.titulo",
      "mappedBy": "egresado"
    },
    "informacion_educativas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::educativa.educativa",
      "mappedBy": "egresado"
    },
    "complementar_informacion_laborals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::laboral.laboral",
      "mappedBy": "egresado"
    }
  }
}
